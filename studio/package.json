{
  "name": "inourhands-studio",
  "private": true,
  "version": "0.1.0",
  "description": "The answers to all problems depend on achieving inner awakening",
  "main": "package.json",
  "author": "Matt Cook <matt@sciencefidelity.co.uk>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity start",
    "start": "sanity start",
    "build": "sanity build",
    "clean": "rm -rf dist",
