    "@sanity/core": "^2.31.0",
    "@sanity/default-layout": "^2.31.0",
    "@sanity/default-login": "^2.31.0",
    "@sanity/desk-tool": "^2.31.0",
    "@sanity/vision": "^2.31.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "styled-components": "^5.3.5"
  }
